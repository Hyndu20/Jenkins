
//Jenkinsfile (Declarative Pipeline)
pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                echo 'this is build'
            }
        }
        stage('Test') {
            steps {
                echo 'this is test'
            }
        }
        stage('deploy') {
            steps {
                echo 'this is deploy'
            }
        }
    }
}

